{
  "name": "@dragonza/react-flip-card",
  "version": "1.0.0",
  "description": "A Simple React flip card",
  "dependencies": {
    "classnames": "^2.2.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "prop-types": "^15.7.2",
    "react-scripts": "3.1.2",
    "react-test-renderer": "^16.9.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "author": {
    "name": "dragonza"
  },
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "set NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src/ --out-dir dist --copy-files",
